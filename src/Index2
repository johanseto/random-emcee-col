const messages = [
    "Chang",
    "Gaviria",
    "Carpediem",
    "Eleven",
    "Letra",
    "Ñeko",
    "Big-killa",
    "RBN",
    "Loquillo",
    "Filosofo",
    "Airon",
    "Valles-t",
]

//Crear function to get random messages
const randomMsg = () => {
    const message = messages[Math.floor(Math.random()*messages.length)]
    console.log(`the winner of the fms 2022 would be ${message}`)
}

// Exportar como un módulo

module.exports = {randomMsg}
